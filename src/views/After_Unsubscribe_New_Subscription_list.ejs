<% include partials/_header %>

<hr>
    <div>  <a href="/" class="btn btn-info" >Home</a> </div> 
<hr>

 <p align="center"  style="color:red;" >After Unsubscribe   User's current  Subscription services List  </p> <br>

 
		<div >
            <table  width="80%" style="margin-left:10%;">
               
                <tbody>
                <% if (data) { %>
                    <% for(var i = 0; i < data.length; i++) { %>
                       
                           
						  <tr>        
							 <td>Name</td>						
						      <td><%= data[i].name %></td>
							</tr>
						<tr>							
					     <td>Balance</td>
							  <td><%= data[i].balance %></td>
							  </tr>
						<tr>
							   <td>Contact</td>							  
							    <td><%= data[i].phone %></td>
								
						</tr>
							
							
							
                           <% if (data[i].sports == "on") { %>		
								<tr>
								<td>Sports</td>							   
						    <td><%= data[i].sports %></td>
							</tr>
						      <% } %>							
							
							
                            <% if (data[i].news == "on") { %>	
							<tr>
							 <td>News</td>							  
						    <td><%= data[i].news %></td>
							</tr>
						      <% } %>
							  
						   <% if (data[i].health == "on") { %>	
							<tr>
							 <td>Health</td>							  
						    <td><%= data[i].health %></td>
						  </tr>
						      <% } %>
							  
                             <% if (data[i].namaz == "on") { %>	
							<tr>						   
						   	<td>Salat</td>							  
						    <td><%= data[i].namaz %></td>
							   </tr>
						      <% } %>
							
							
							  	<tr>
                            <td>Update</td>	
                            <td>    <a href="/update_subscription/<%= data[i].id %>" class="btn btn-info">Edit Subscription</a>   </td>						
							
                        </tr>
						
						 	<tr>
                            <td>delete_subscription</td>	
                            <td>     <a href="/delete_subscription/<%= data[i].id %>" class="btn btn-danger">Delete</a>  </td>						
							
                        </tr>
                           
                         
                    <% } %>
                <% } %>
                </tbody>
            </table>
        </div>
		
		<br>
		


<% include partials/_footer %>
